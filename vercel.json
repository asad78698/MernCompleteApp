{
    "version": 2,
    "builds": [
      {
        "src": "backend/app.js",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/moviewebsite/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "frontend/moviewebsite/build"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/backend/app.js"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/moviewebsite/$1"
      }
    ]
  }
  